# Redis (same as mobius-chat)
REDIS_URL=redis://localhost:6379/0

# Scraper queue keys (defaults work with shared Redis)
SCRAPER_REQUEST_KEY=mobius:scraper:requests
SCRAPER_RESPONSE_KEY_PREFIX=mobius:scraper:response:
SCRAPER_RESPONSE_TTL_SECONDS=86400

# GCS (reuse mobius-config or mobius-rag bucket)
GCS_BUCKET=mobius-rag-uploads-mobiusos

# Delay (seconds) before each HTTP request to the target site (reduces traffic)
REQUEST_DELAY_SECONDS=1.0

# Tree scan limits
TREE_MAX_DEPTH=3
TREE_MAX_PAGES=50
SCOPE_MODE=same_domain

# Document types to download (comma-separated)
DOCUMENT_TYPES=pdf,doc,docx,xls,xlsx,ppt,pptx

# Summary (optional): set one for include_summary=true to work
# OPENAI_API_KEY=sk-...
# VERTEX_PROJECT_ID=your-project
# GOOGLE_APPLICATION_CREDENTIALS=credentials/your-sa.json
